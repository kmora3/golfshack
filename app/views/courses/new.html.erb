
  <%=bootstrap_form_for @course, url: courses_path do |c| %>
  <%=c.text_area :name %>
  <%=c.text_area :description %>
  <%=c.text_area :address %>
  <%=c.text_area :city %>
  <%=c.text_area :state %>
  <%=c.text_area :zipcode %>
  <%=c.text_area :phone %>
  <%=c.text_area :hole_count %>
  <div class="form-group">
    <%= c.file_field :photo, class: "form-control" %>
  </div>

  <%=c.submit class: "btn btn-primary btn-lg"%>
<% end %>
